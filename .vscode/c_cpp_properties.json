{
  "configurations": [
    {
      "name": "macos-native-jsi",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/node_modules/react-native/React",
        "${workspaceFolder}/node_modules/react-native/ReactCommon"
      ],
      "defines": [],
      "macFrameworkPath": [
        "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/System/Library/Frameworks"
      ],
      "compilerPath": "/usr/bin/clang++",
      "cStandard": "c17",
      "cppStandard": "c++20",
      "intelliSenseMode": "macos-clang-x64"
    },
    {
      "name": "macos-wasm-emscripten",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/apps/web/wasm",
        "${env:EMSDK}/upstream/emscripten/system/include",
        "${env:EMSDK}/upstream/emscripten/cache/sysroot/include",
        "/opt/homebrew/Cellar/emscripten/4.0.12/libexec/system/include",
        "/opt/homebrew/Cellar/emscripten/4.0.12/libexec/cache/sysroot/include"
      ],
      "defines": ["__EMSCRIPTEN__", "EMSCRIPTEN"],
      "macFrameworkPath": [
        "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/System/Library/Frameworks"
      ],
      "compilerPath": "/opt/homebrew/bin/em++",
      "cStandard": "c17",
      "cppStandard": "c++20",
      "intelliSenseMode": "macos-clang-x64"
    }
  ],
  "version": 4
}
